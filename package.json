    "nodemon": "^1.18.3",
    "parcel-bundler": "^1.9.7"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "dotenv-safe": "^6.0.0",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "newsapi": "^2.3.0",
    "react": "^16.4.2",
